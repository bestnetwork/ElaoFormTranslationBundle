{
    "name": "elao/form-translation-bundle",
    "type": "symfony-bundle",
    "description": "Generate translation key for forms",
    "keywords": ["elao", "form", "translation"],
    "homepage": "https://github.com/Elao/ElaoFormTranslationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Thomas Jarrand",
            "homepage": "https://github.com/Tom32i"
        },
        {
            "name": "Damian Dlugosz",
            "homepage": "https://github.com/bigfoot90"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "git@bitbucket.org:bestnetwork/utility.git" }
    ],
    "require": {
        "symfony/framework-bundle": "~2.6",
        "symfony/form": "~2.6",
        "bestnetwork/utility": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Elao\\Bundle\\FormTranslationBundle\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
